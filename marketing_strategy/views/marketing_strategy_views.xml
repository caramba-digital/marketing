<odoo>
  <data>
  
          <!-- Menu items -->
          

        <menuitem name="Marketing Strategy"
            id="marketing_stretegy_menu_root"
            sequence="9"
            groups="base.group_user"
            web_icon="marketing_strategy,static/description/icon.png"/>
            
          <menuitem
            id="marketing_stretegy__menu"
            name="Strategy"
            parent="marketing_stretegy_menu_root"
            sequence="1" groups="group_marketing_strategy_user"/>
            
        <menuitem
            id="marketing_stretegy__menu_assets"
            name="Assets"
            parent="marketing_stretegy_menu_root"
            sequence="3" groups="group_marketing_strategy_user"/>
            
		 <menuitem
            id="marketing_stretegy__menu_solution"
            name="Solutions"
            parent="marketing_stretegy_menu_root"
            sequence="2" groups="group_marketing_strategy_manager"/>
            
		

        <menuitem
            id="marketing_stretegy_menu_config"
            name="Configuration"
            parent="marketing_stretegy_menu_root"
            sequence="80" groups="group_marketing_strategy_manager"/>
            
            
            <record id="marketing_stretegy_consumer_trend_view_form" model="ir.ui.view">
            <field name="name">marketing_stretegy.consumer_trend.form</field>
            <field name="model">marketing_strategy.consumer_trend</field>
            <field name="arch" type="xml">
                <form string="Consumer Trend Canvas">
                	 <sheet>
                	 <div class="oe_title">
                        <h1><field name="name" placeholder="Name" nolabel="1"/></h1>
                       </div>
                       <div class="row">
                       		<div class="col-8">
                       			<h2 class="text-center">ANALYZE</h2>
                       			<div class="row">
                       				<div class="col-9">
                       				
                       				</div>
                       				<div class="col-3">
                       				
                       				</div>
                       			</div>
                       		</div>
                       		<div class="col-4">
                       			<h2 class="text-center">APPLY</h2>
                       		</div>
                	 </div>
                	 </sheet>
                </form>
            </field>
         </record>
         
       <record id='marketing_stretegy_consumer_trend_view_tree' model='ir.ui.view'>
        <field name="name">marketing_stretegy.consumer_trend.tree</field>
        <field name="model">marketing_strategy.consumer_trend</field>
        <field name="arch" type="xml">
            <tree string="Brand">
                <field name="name" />
            </tree>
        </field>
    </record>
    
       <record id='marketing_stretegy_consumer_trend_action' model='ir.actions.act_window'>
        <field name="name">Consumer Trend</field>
        <field name="res_model">marketing_strategy.consumer_trend</field>
        <field name="view_mode">kanban,tree,form</field>
        <field name="help" type="html">
          <p class="o_view_nocontent_smiling_face">
            Create a new consumer trend canvas
          </p>
        </field>
    </record>

    <menuitem action="marketing_stretegy_consumer_trend_action" parent="marketing_stretegy__menu" id="Marketing_stretegy_consumer_trend_action_menu" sequence="2"/>
         
  
  
  </data>
</odoo>