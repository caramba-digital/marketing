<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE xml>
<odoo noupdate="1">

    <!--Group for paintball user -->
    <record id="group_marketing_strategy_user" model="res.groups">
        <field name="name">Marketing Strategy / User</field>
        <field eval="[(4, ref('base.user_root'))]" name="implied_ids" />
    </record>

    <!--Group for paintball manager -->
    <record id="group_marketing_strategy_manager" model="res.groups">
        <field name="name">Marketing Strategy/ Manager</field>
        <field eval="[(4, ref('marketing_strategy.group_marketing_strategy_user'))]" name="implied_ids" />
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>